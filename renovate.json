{
  "enabled": true,
  "schedule": [
    "after 10pm every weekday",
    "before 5am every weekday",
    "every weekend"
  ],
  "extends": [
    "config:best-practices"
  ],
  "postUpdateOptions": [
    "gomodTidy",
    "gomodMassage"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    }
  ],
  "prConcurrentLimit": 2
}
